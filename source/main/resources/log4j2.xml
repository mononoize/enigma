<?xml version = "1.0" encoding = "UTF-8"?>
<Configuration status = "WARN">
	<Appenders>
		<Console name = "Console" target = "SYSTEM_OUT">
			<PatternLayout pattern = "%d{HH:mm:ss.SSS}  %5.5p  %-20c{1.}  %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level = "info">
			<AppenderRef ref = "Console"/>
		</Root>
	</Loggers>
</Configuration>